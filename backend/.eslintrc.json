{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/@typescript-eslint",
    "prettier/babel",
    "prettier/flowtype",
    "prettier/react",
    "prettier/standard",
    "prettier/unicorn"
  ],
  "rules": {
    "eqeqeq": "off",
    "class-methods-use-this": "off"
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}
